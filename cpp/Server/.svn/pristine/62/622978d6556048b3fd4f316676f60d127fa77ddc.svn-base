syntax = "proto3";
option optimize_for = SPEED;
option java_package = "com.dream.cube.tcp";
/*
C++_TYPE	proto_TYPE
float		float
char*		string
bytes		string
int8		int32
int		    int32
int16		int32
int32		int32
uint8		uint32
uint16		uint32
uint32		uint32
int64		int64
uint64		uint64
bool 		bool
string 		string
*/

/*
repeated	array
*/

/*
protoc --cpp_out=.  this.proto
*/

//性别
enum e_GenderType
{
	e_Gender_Unknown= 0;	//未知
	e_Gender_Male	= 1;	//男
	e_Gender_Female	= 2;	//女
}

//消息类型
enum e_MsgType
{
    MSGTYPE_TEXT = 0;   //文字
    MSGTYPE_IMAGE_WEIXIN = 1;  //从微信来的图片
    MSGTYPE_VOICE_WEIXIN = 2;  //从微信来的语音
    MSGTYPE_VEDIO = 3;  //视频
    MSGTYPE_REDPACK = 4;//红包
    MSGTYPE_ASSIST = 5; //群助手
    MSGTYPE_SYSTEMINFO = 6;	//魔方团队
	MSGTYPE_REMIND = 7;	//提示消息
	MSGTYPE_RECALL = 8;	//消息撤回
	MSGTYPE_LINKS = 9; //图文链接
	MSGTYPE_STICKER = 10; //表情
	MSGTYPE_TIP	= 11;			//送礼
	MSGTYPE_IMAGE = 12;  //图片
  MSGTYPE_VOICE = 13;  //语音
  MSGTYPE_UPLOADWALL = 14;  //上墙
  MSGTYPE_CLOSE_LIVE_COURSE = 15;  //关闭课程、直播间通知
  MSGTYPE_PPT_VOICE = 16;  //PPT课程发语音
  MSGTYPE_CANCELL_KEYPOINT = 17;  //取消重点或者重新划重点，事后操作的
}

enum e_RemindType
{
	REMIND_TAKE_REDPACKET = 0;		//领取红包提示
	REMIND_BE_TAKEN_REDPACKET = 1;	//红包被领取提示
	REMIND_JOINGROUP = 2;			//新成员加群
	REMIND_ROLECHANGE = 3;			//角色变更
}

message CMDString
{
	string	data		= 1;	//封装成CMDString
}

message CMDUInt32
{
	uint32	data		= 1;	//封装成CMDUInt32
}

message UserInfo_t
{
	uint32 userId		= 1;	//用户id
	string alias		= 2;	//用户昵称
	string head			= 3;	//用户头像
	uint32 roleType		= 4;	//用户等级
	uint32 authState	= 5;	//认证状态
	e_GenderType gender	= 6;	//性别 
	uint32 UserType=7;//是否助教1是助教0不是
}

message CMDErrCode
{
	uint32 errID		= 1;	//ERROR CODE
	string errMsg		= 2;	//ERROR MSG
	repeated string paramValue = 3;	//ERROR ParamValue
}

